{
  "name": "fos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "multiscript yarn + \"cd client && yarn\" + \"cd server && yarn\"",
    "dev": "cross-env NODE_ENV=development multiscript yarn server:dev + yarn client:dev",
    "start": "cross-env NODE_ENV=production yarn vue:build && yarn server:start",
    "server:dev": "cd server && ts-node-dev src",
    "server:start": "cd server && tsc && node dist",
    "client:dev": "cd client && webpack-dev-server --mode=development",
    "client:build": "cd client && webpack --mode=production"
  },
  "dependencies": {
    "@andrewrivers/simple_webrtc": "^0.4.3",
    "chalk": "^4.0.0",
    "express": "^4.17.1",
    "localforage": "^1.7.3",
    "markdown-it": "^10.0.0",
    "nedb-promises": "^4.0.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "register-service-worker": "^1.7.1",
    "typescript": "~3.8.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.7.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "multiscript": "^0.1.8",
    "node-sass": "^4.13.1",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^4.0.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "gitHooks": {
    "pre-commit": ""
  }
