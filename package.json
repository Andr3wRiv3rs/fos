{
  "name": "fos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development multiscript yarn express:dev + yarn vue:dev",
    "start": "cross-env NODE_ENV=production yarn build && yarn start",
    "express:dev": "nodemon -q index.js",
    "express:start": "node index.js",
    "vue:dev": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "precommit": "yarn lint",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@andrewrivers/simple_webrtc": "^0.4.2",
    "chalk": "^4.0.0",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "localforage": "^1.7.3",
    "markdown-it": "^10.0.0",
    "nedb-promises": "^4.0.1",
    "prismjs": "^1.20.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-pwa": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "multiscript": "^0.1.8",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.3",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.2",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
