    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "vue-eslint-parser": "^7.0.0",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
