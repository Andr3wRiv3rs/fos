{
  "name": "fos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development multiscript yarn server:dev + yarn client:dev",
    "build": "cross-env NODE_ENV=production yarn server:build && yarn client:build",
    "start": "node .",
    "server:dev": "ts-node-dev server",
    "server:build": "tsc",
    "client:dev": "cd client && webpack-dev-server --mode=development --display=minimal",
    "client:build": "cd client && webpack --mode=production",
    "lint": "eslint --fix --ext .js,.vue,.ts server client/src"
  },
  "dependencies": {
    "@andrewrivers/simple_webrtc": "^0.4.3",
    "chalk": "^4.0.0",
    "express": "^4.17.1",
    "localforage": "^1.7.3",
    "markdown-it": "^10.0.0",
    "nedb-promises": "^4.0.1",
    "prismjs": "^1.20.0",
    "register-service-worker": "^1.7.1",
    "ts-node": "^8.9.0",
    "typescript": "~3.8.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "multiscript": "^0.1.8",
    "node-sass": "^4.13.1",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^4.0.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
