    "markdown-it": "^10.0.0",
    "nedb-promises": "^4.0.1",
    "prismjs": "^1.20.0",
    "register-service-worker": "^1.7.1",
    "ts-node": "^8.9.0",
    "typescript": "~3.8.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-pwa": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
